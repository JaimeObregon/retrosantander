{
  "name": "retrosantander",
  "private": true,
  "description": "Un experimento de Jaime Gómez-Obregón con los archivos fotográficos públicos.",
  "homepage": "https://github.com/JaimeObregon/retrosantander",
  "bugs": {
    "url": "https://github.com/JaimeObregon/retrosantander/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JaimeObregon/retrosantander.git"
  },
  "license": "Véase el fichero LICENSE",
  "author": "Jaime Gómez-Obregón",
  "type": "module",
  "scripts": {
    "build": "bin/build.sh",
    "serve": "bin/serve.sh",
    "serve:guregipuzkoa": "yarn run serve guregipuzkoa 8002",
    "serve:retrogipuzkoa": "yarn run serve retrogipuzkoa 8001",
    "serve:retrosantander": "yarn run serve retrosantander 8000"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "stylelint": {
    "overrides": [
      {
        "files": [
          "*.js"
        ],
        "customSyntax": "postcss-styled-syntax"
      }
    ],
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-recess-order"
    ],
    "rules": {
      "no-descending-specificity": null
    }
  },
  "devDependencies": {
    "@aws-sdk/client-rekognition": "^3.896.0",
    "@aws-sdk/client-s3": "^3.896.0",
    "@types/node": "^24.5.2",
    "esbuild": "^0.25.10",
    "exiftool": "^0.0.3",
    "html-entities": "^2.6.0",
    "netlify": "^23.7.3",
    "node-fetch": "^3.3.1",
    "postcss": "^8.5.6",
    "postcss-styled-syntax": "^0.7.1",
    "prettier": "^3.6.2",
    "stylelint": "^16.24.0",
    "stylelint-config-recess-order": "^7.3.0",
    "stylelint-config-standard": "^39.0.0",
    "stylelint-order": "^7.0.0"
  },
  "engines": {
    "node": ">=20.12.0"
  }
}
