{
  "name": "retrosantander",
  "private": true,
  "description": "Un experimento de Jaime Gómez-Obregón con los archivos fotográficos públicos.",
  "homepage": "https://github.com/JaimeObregon/retrosantander",
  "bugs": {
    "url": "https://github.com/JaimeObregon/retrosantander/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JaimeObregon/retrosantander.git"
  },
  "license": "Véase el fichero LICENSE",
  "author": "Jaime Gómez-Obregón",
  "type": "module",
  "scripts": {
    "build": "bin/build.sh",
    "guregipuzkoa": "bin/serve.sh guregipuzkoa",
    "retrogipuzkoa": "bin/serve.sh retrogipuzkoa",
    "retrosantander": "bin/serve.sh retrosantander"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "stylelint": {
    "overrides": [
      {
        "files": [
          "*.js"
        ],
        "customSyntax": "postcss-styled-syntax"
      }
    ],
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-recess-order"
    ],
    "rules": {
      "no-descending-specificity": null
    }
  },
  "engines": {
    "node": ">=20.12.0"
  },
  "devDependencies": {
    "@aws-sdk/client-rekognition": "^3.883.0",
    "@aws-sdk/client-s3": "^3.883.0",
    "@types/node": "^24.3.1",
    "esbuild": "^0.25.9",
    "exiftool": "^0.0.3",
    "html-entities": "^2.6.0",
    "netlify": "^23.5.0",
    "node-fetch": "^3.3.1",
    "postcss-styled-syntax": "^0.7.1",
    "prettier": "^3.6.2",
    "stylelint": "^16.23.1",
    "stylelint-config-recess-order": "^7.3.0",
    "stylelint-config-standard": "^39.0.0"
  }
}
